            </div>
            <div id="test-results-summary-overall-graph-canvas" style="display: flex;"></div>
        </div>
    </div>
    <div class="test-results-table-container">
        <div class="aui-nav-heading" title="$i18n.getText('reportsview.div.aui-nav-heading.tooltip.stuff')" style="margin: 5px 0;">
            <strong style="color: #000;">$i18n.getText("reportsview.label.execution-cycle-details")</strong>
        </div>
        <div class="test-results-table-header">
            <table class="aui controller">
                <tr data-level="header" class="header" style="font-weight: 600;">
                    <td width="70px;" style="min-width: 70px; border-bottom: none;"></td>
                    <td width="15%" style="border-bottom: none;">
                        <span>$i18n.getText("reportsview.table.td.span.execution-cycle")</span>
                        <!--<button id="test-reports-filter-btn" class="aui-button" style="outline: none; padding: 5px; font-size: 12px;">-->
                        <!--<span class="aui-icon aui-icon-small aui-iconfont-filter"></span>-->
                        <!--<span>Filter</span>-->
                        <!--</button>-->
                    </td>
                    <td width="18%" style="border-bottom: none;">$i18n.getText("reportsview.table.td.details")</td>
                    <td width="10%" style="border-bottom: none;">$i18n.getText("reportsview.table.td.execution-date")</td>
                    <td width="14%" style="text-align: center; border-bottom: none;">$i18n.getText("reportsview.table.td.total")</td>
                    <td width="8%" style="border-bottom: none;">$i18n.getText("reportsview.table.td.status")</td>
                    <td width="7%" style="border-bottom: none;">$i18n.getText("reportsview.table.td.state")</td>
                    <td width="14%" style="text-align: center; border-bottom: none;">$i18n.getText("reportsview.table.td.completed")</td>
                    <td width="14%" style="text-align: center; border-bottom: none;">$i18n.getText("reportsview.table.td.pending")</td>
                </tr>
