                        <div class="aui-nav-heading" style="margin: 5px 0 0;">
                            <strong style="color: #000;">$i18n.getText("reportsview.label.total-test-cases")</strong>
                        </div>
                        <div class="total-count">
                            <img src="$req.contextPath/download/resources/com.virtusa.gto.plugins.aitest:aitest-resources/images/test-management/total.svg"
                                 alt="Total">
                            <span id="current-total-tc-count">-</span>
                        </div>
                    </div>
                    <div class="card executed">
                        <div class="aui-nav-heading" style="margin: 5px 0 0;">
                            <strong style="color: #000;">$i18n.getText("reportsview.label.executed-test-cases")</strong>
                        </div>
                        <div class="executed-types">
                            <div class="pass">
                                <div class="aui-nav-heading" style="margin: 5px 0 0;">
                                    <strong style="color: #000;">$i18n.getText("reportsview.label.executed-test-cases-pass")</strong>
                                </div>
                                <div class="pass-count">
                                    <img src="$req.contextPath/download/resources/com.virtusa.gto.plugins.aitest:aitest-resources/images/test-management/pass.svg"
