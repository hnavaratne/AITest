        <section  class="aui-page-panel-content" id="test-case-details-page-panel" style="display:none">
                <header class="aui-page-header">
                    <div class="aui-page-header-inner">
                        <span class="aui-icon aui-icon-large aui-iconfont-bullet-list"></span>
                        <span class="header-title" id="selected-test-case-titile"></span>
                    </div>
                </header>
                <div class="aui-tabs horizontal-tabs">
                    <ul class="tabs-menu">
                        <!--Test case details-->
                        <li class="menu-item active-tab">
                            <a href="#test-case-details-tab">
                                <span class="aui-icon aui-icon-small aui-iconfont-info-circle"></span>
                                $i18n.getText("designing-view.label.details")
                            </a>
                        </li>
                        <!--Test case data-->
                        <li class="menu-item">
                            <a href="#test-case-data-tab">
                                <span class="aui-icon aui-icon-small aui-iconfont-heading-row"></span>
                                $i18n.getText("designing-view.label.test-data")
                            </a>
                        </li>
                        <!--Test case scripts-->
                        <li class="menu-item">
                            <a href="#test-case-scripts-tab">
                                <span class="aui-icon aui-icon-small aui-iconfont-documents"></span>
                                $i18n.getText("designing-view.label.script-details")
                            </a>
                        </li>
                        <!--linked issues added-->
                        <li class="menu-item">
                            <a href="#test-case-linkedissue-tab">
                                <span class="aui-icon aui-icon-small aui-iconfont-issues"></span>
                                $i18n.getText("designing-view.label.linked-user-stories")
                            </a>
                        </li>
                    </ul>
                    <!-- Test Case Details tab start -->
                    <div class="tabs-pane active-pane" id="test-case-details-tab">
                        <form class="aui">
                            <div class="field-group">
                                <label for="test-case-name">
                                    $i18n.getText("all.label.name")
                                    <span class="aui-icon icon-required">(required)</span>
                                </label>
                                <input class="text medium-field" type="text" id="test-case-name" name="test-case-name"
                                    placeholder="Test Case Name" disabled>
                                <div class="description">$i18n.getText("reportsview.div.your-test-case-name")</div>
                            </div>
                            <div class="field-group">
                                <label for="test-case-description">
                                    $i18n.getText("reportsview.label.description")
                                </label>
                                <textarea class="textarea" name="test-case-description" id="test-case-description" rows="5"
                                        placeholder="Your description here..."></textarea>
                            </div>
                            <div class="field-group">
                                <label for="test-case-overall-expected-result">
                                    $i18n.getText("reportsview.label.overall-expected-result")
                                </label>
                                <textarea class="textarea" name="test-case-overall-expected-result"
                                        id="test-case-overall-expected-result" rows="5"
                                        placeholder="Overall Expected Result here..."></textarea>
                            </div>
                            <div class="field-group">
                                <label>$i18n.getText("reportsview.label.automated")</label>
                                <div class="radio">
                                    <input class="radio" type="radio" name="automatedRadioButtons" id="automatedYes"
                                        value="Yes">
                                    <label for="automatedYes">$i18n.getText("reportsview.label.yes")</label>
                                </div>
                                <div class="radio">
                                    <input class="radio" type="radio" checked="checked" name="automatedRadioButtons"
                                        id="automatedNo" value="No">
                                    <label for="automatedNo">$i18n.getText("reportsview.label.no")</label>
                                </div>
                            </div>
                            <div class="field-group">
                                <label>$i18n.getText("reportsview.label.manual")</label>
                                <div class="radio">
                                    <input class="radio" type="radio" checked="checked" name="manualRadioButtons"
                                        id="manualYes" value="Yes">
                                    <label for="manualYes">$i18n.getText("reportsview.label.yes")</label>
                                </div>
                                <div class="radio">
                                    <input class="radio" type="radio" name="manualRadioButtons" id="manualNo" value="No">
                                    <label for="manualNo">$i18n.getText("reportsview.label.no")</label>
                                </div>
                            </div>
                            <div class="field-group steps-group">
                                <div class="aui-tabs horizontal-tabs">
                                    <ul class="tabs-menu">
                                        <li class="menu-item active-tab" id="steps-tab">
                                            <a href="#tab-test-step">$i18n.getText("designing-view.label.steps")</a>
                                        </li>
                                        <li class="menu-item" id="bdt-step-details" style="visibility: hidden">
                                            <a href="#tab-bdt-steps">$i18n.getText("designing-view.label.business-function")</a>
                                        </li>
                                    </ul>
                                    <div class="tabs-pane active-pane" id="tab-test-step"
                                        style="height:calc(50vh);overflow:auto;">
                                        <div class="steps-table-content">
                                            <div class="table-header">
                                                <table>
                                                    <thead>
                                                    <tr>
                                                        <th class="no">$i18n.getText("reportsview.label.no")</th>
                                                        <th class="desc">$i18n.getText("designing-view.label.step-description")</th>
                                                        <th class="result">$i18n.getText("designing-view.label.expected-result")</th>
                                                        <th class="data">$i18n.getText("all.label.data")</th>
                                                    </tr>
                                                    </thead>
                                                </table>
                                            </div>
                                            <div class="table-content">
                                                <table context-menu data-target="menu-1" context-menu-close="resetIndex()">
                                                    <tbody id="test-step-tb">
                                                    <tr ng-repeat="step in steps" ng-class="{'row-selected':($index  == index)}">
                                                        <td class="no" ng-right-click="setSelectedIndex($index)" >{{ $index +
                                                            1 }}
                                                        </td>
                                                        <td class="desc" ng-right-click="setSelectedIndex($index)" ng-click="setClickedIndex($index,0)">
                                                            <textarea ng-mouseup="resize($event)" ng-mousedown="resize($event)" ng-if="isBCFunction(step.function) == false" paste-excel class="textarea-desc" type="text" ng-model="step.step" style="max-width: 100% !important;height: 100%;" ng-keyup="mention($event)" bcid={{step.function}} ></textarea>
                                                            <input ng-if="isBCFunction(step.function) == true" ng-keyup="mention($event)" type="text" ng-model="step.step" style="max-width: 100% !important;height: 100%;" bcid={{step.function}}>
                                                        </td>
                                                        <td ng-if="isBCFunction(step.function) == false" class="result" ng-right-click="setSelectedIndex($index)" ng-click="setClickedIndex($index,1)">
                                                            <textarea ng-mouseup="resize($event)" ng-mousedown="resize($event)" paste-excel class="textarea-result" type="text" ng-model="step.expectedResult" style="max-width: 100% !important;height: 100%;font-family: inherit;"></textarea>
                                                        </td>
                                                        <td ng-if="isBCFunction(step.function) == true" align="center" class="result" ng-right-click="setSelectedIndex($index)" ng-click="setClickedIndex($index,1)" style="vertical-align: middle;">
                                                            <a  href ng-click="viewSteps(step.function)" vid={{step.function}} class="view-Steps" ng-if="isBCFunction(step.function) == true"><span class="aui-lozenge aui-lozenge-moved"> View Business Function </span> </a>
                                                        </td>
                                                        <td class="data" ng-right-click="setSelectedIndex($index)" ng-click="setClickedIndex($index,2)">
                                                            <textarea ng-mouseup="resize($event)" ng-mousedown="resize($event)" ng-if="isBCFunction(step.function) == false" paste-excel class="textarea-data" type="text" ng-model="step.data" style="max-width: 100% !important;height: 100%;font-family: inherit;" stepId = {{step.id}} ng-change="onStepParamChange()"></textarea>
                                                            <input ng-if="isBCFunction(step.function) == true" type="text" style="max-width: 100% !important;height: 100%;font-family: inherit;" disabled>
                                                        </td>
                                                        <div class="jira-mention" style="display:none;">
                                                            <ul class="jira-scrollable-menu">
                                                                <li class="jira-mention-menu-item"
                                                                    ng-repeat="element in elements"
                                                                    ng-click="mentionSelect($event)" scid={{element.Id}}>{{element.Name}}</li>
                                                            </ul>
                                                        </div>
                                                    </tr>
                                                    <tr>
                                                        <td class="no"></td>
                                                    <td class="desc" ng-click="setClickedIndex(-1,0)">
                                                            <textarea ng-if="isBCFunction(newstep.function) == false" paste-excel class="textarea-desc" type="text" ng-model="newstep.step" style="max-width: 100% !important;height: 100%;margin-bottom: 1px;font-family: inherit;" ng-keyup="mention($event)" bcid="" ng-enter="addStep()" ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                            ></textarea>
                                                            <input ng-if="isBCFunction(newstep.function) == true" ng-keyup="mention($event)" type="text" ng-model="newstep.step" style="max-width: 100% !important;height: 100%;font-family: inherit;font-family: inherit;" ng-enter="addStep()">
                                                        </td>
                                                        <td ng-if="isBCFunction(newstep.function) == false" class="result" ng-click="setClickedIndex(-1,1)" ng-enter="addStep()">
                                                            <textarea paste-excel class="textarea-result" type="text" ng-model="newstep.expectedResult" style="max-width: 100% !important;height: 100%;margin-bottom: 1px;font-family: inherit;" ng-enter="addStep()" ng-mouseup="resize($event)" ng-mousedown="resize($event)"></textarea>
                                                        </td>
                                                        <td ng-if="isBCFunction(newstep.function) == true" class="result" ng-click="setClickedIndex(-1,1)" style="vertical-align: middle;" ng-enter="addStep()">
                                                            <a href="#" ng-if="isBCFunction(newstep.function) == true"><span class="aui-lozenge aui-lozenge-moved" ng-enter="addStep()" > View Business Function </span> </a>
                                                        </td>
                                                        <td class="data" ng-click="setClickedIndex(-1,2)">
                                                            <textarea ng-if="isBCFunction(newstep.function) == false" paste-excel class="textarea-data" type="text" ng-model="newstep.data" style="max-width: 100% !important;height: 100%;margin-bottom: 1px;font-family: inherit;" ng-enter="addStep()" ng-mouseup="resize($event)" ng-mousedown="resize($event)"></textarea>
                                                            <input ng-if="isBCFunction(newstep.function) == true" type="text" style="max-width: 100% !important;height: 100%;margin-bottom: 1px;font-family: inherit;" ng-enter="addStep()" disabled>
                                                        </td>
                                                    </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                            <div class="dropdown position-fixed" id="menu-1">
                                                <ul class="v-dropdown-menu" role="menu">
                                                    <li ng-click="clickedDelete()"><a class="pointer" role="menuitem" tabindex="1"><span class="aui-icon aui-icon-small aui-iconfont-trash" ></span>$i18n.getText("all.label.delete")</a></li>
                                                    <li ng-click="insertNewRow()"><a class="pointer" role="menuitem" tabindex="1"><span class="aui-icon aui-icon-small aui-iconfont-insert-row-after"></span> Insert Row Down</a></li>
                                                    <li ng-click="insertNewRowUp()"><a class="pointer" role="menuitem" tabindex="1"><span class="aui-icon aui-icon-small aui-iconfont-insert-row-after"></span> Insert Row Up</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="tabs-pane" id="tab-bdt" style="height:330px;">
                                        <div id="sticker">
                                            <ul id="sortable">
                                                <li class="">When</li>
                                                <li class="">Then</li>
                                                <li class="">Given</li>
                                                <li class="">Scenario</li>
                                                <li class="">Feature</li>
                                            </ul>
                                        </div>
                                        <div id="bdt-table">
                                            <table class="aui" id="droppable">
                                                <thead>
                                                <tr>
                                                    <th style="width:2%;"></th>
                                                    <th>$i18n.getText("designing-view.label.description")</th>
                                                </tr>
                                                </thead>
                                                <tbody id="sticker-table-body">

                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="tabs-pane" id="tab-bdt-steps" style="height:330px; visibility: hidden">
                                        <div class="steps-table-content" id="bdt-steps-table-content">
                                            <div class="table-header">
                                                <table>
                                                    <thead>
                                                    <tr>
                                                        <th class="no">$i18n.getText("reportsview.label.no")</th>
                                                        <th class="desc">$i18n.getText("designing-view.label.step-description")</th>
                                                        <th class="result">$i18n.getText("designing-view.label.expected-result")</th>
                                                        <th class="data">$i18n.getText("all.label.data")</th>
                                                    </tr>
                                                    </thead>
                                                </table>
                                            </div>
                                            <div class="table-content">
                                                <table context-menu data-target="menu-1" context-menu-close="resetIndex()">
                                                    <tbody id="bdt-test-step-tb">
                                                                <tr ng-repeat="bdtstep in bdtSteps"
                                                        ng-class="{'row-selected':($index  == index)}">
                                                        <td class="no" ng-right-click="setSelectedIndex($index)">{{ $index +
                                                            1 }}
                                                        </td>
                                                        <td class="desc">
                                                            <textarea type="text" ng-model="bdtstep.step"
                                                                    class="textarea-desc"
                                                                    style="max-width: 100% !important;height: 100%;margin-bottom: 1px;"
                                                            ></textarea>
                                                        </td>
                                                        <td class="result">
                                                            <textarea class="textarea-result" type="text"
                                                                    ng-model="bdtstep.expectedResult"
                                                                    style="max-width: 100% !important;height: 100%;margin-bottom: 1px;"></textarea>
                                                        </td>
                                                        <td class="data" >
                                                            <textarea class="textarea-data"
                                                                    style="max-width: 100% !important;height: 100%;margin-bottom: 1px;" ng-model="bdtstep.data"></textarea>
                                                        </td>

                                                    </tr>
                                                    </tbody>
                                                </table>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="buttons-container">
                                <div class="buttons">
                                    <button class="button submit" id="test-case-form-save-btn" ng-click="save()">$i18n.getText("all.label.save")
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <!-- Test Case Details tab end -->

                    <!-- Test Case Data tab start -->
                    <div class="tabs-pane" id="test-case-data-tab">
                        <div class="test-case-data-management" id="test-case-data-management">
                            <div class="filter">
                                <!--<button class="aui-button" id="importDataFromExcelBtn">Import from Excel</button>-->
                                <button class="aui-button" id="importDataFromOscarBtn">$i18n.getText("designing-view.label.auto-generate-from-ai")</button>
                            </div>
                            <div style="height:600px;overflow:scroll;">
                                <div class="data-table" id="testdata">
                                    <table class="aui" id="test-data-table" style="">
                                        <thead id="auithead">

                                        </thead>
                                        <tbody id="auitbody">

                                        </tbody>
                                    </table>
                                </div>
                                <div style="height:400px;">
                                    <div class="data-table">

                                        <table class="aui" id="test-data-table2" style="">
                                            <tbody id="auitbody-data">

                                            </tbody>
                                        </table>

                                    </div>
                                </div>
                            </div>
                            <div class="filter">
                                <button id="save-data" class="aui-button aui-button-primary">$i18n.getText("all.label.save")</button>
                            </div>
                        </div>
                    </div>
                    <!-- Test Case Data tab end -->

                    <!--Start of Scripts Pane-->
                    <div class="tabs-pane" id="test-case-scripts-tab">
                        <div id="test-case-scripts-management">
                            <div class="data-table" id="test-case-scripts-table">
                                <table class="display compact" style="width:100%" id="script-table">
                                    <thead>
                                    <tr>
                                        <th id="script-id">$i18n.getText("designing-view.label.script-id")</th>
                                        <th id="script-name">$i18n.getText("designing-view.label.script-name")</th>
                                    </tr>
                                    </thead>
                                    <tbody id="script-table-body">
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <!--End of Scripts Pane-->

                    <!--Linked Issue Pane start-->
                    <div class="tabs-pane" id="test-case-linkedissue-tab">
                        <div id="test-case-linkedissue-management">
                                <div class="link-issue-table-content">
                                    <div class="table-header">
                                        <table>
                                            <thead>
                                            <tr>
                                                <th class="issueid">$i18n.getText("designing-view.label.issue-id")</th>
                                                <th class="issuekey">$i18n.getText("designing-view.label.issue-key")</th>
                                                <th class="summary">$i18n.getText("designing-view.label.summary")</th>
                                            </tr>
                                            </thead>
                                        </table>
                                    </div>
                                    <div class="table-content">
                                        <table context-menu data-target="menu-3" context-menu-close="resetIndex()">
                                            <tbody>
                                            <tr ng-repeat="issue in issues"
                                                ng-class="{'row-selected':($index  == index)}">
                                                <td class="issueid" ng-model="issue.issue_id" ng-right-click="setSelectedIndex($index)"><a href="javascript:void(0);" ng-click="viewUserStory(issue.issue_name)">{{issue.issue_id}}</a></td>
                                                <td class="issuekey" ng-model="issue.issue_name" ng-right-click="setSelectedIndex($index)">{{issue.issue_name}}</td>
                                                <td class="summary" ng-model="issue.summary" ng-right-click="setSelectedIndex($index)">{{issue.summary}}</td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="dropdown position-fixed" id="menu-3">
                                        <ul class="v-dropdown-menu" role="menu">
                                            <li ng-click="clickedDeleteLink()"><a class="pointer" role="menuitem" tabindex="1"><span class="aui-icon aui-icon-small aui-iconfont-trash" ></span>$i18n.getText("all.label.delete")</a></li>
                                        </ul>
                                    </div>
                                </div>
                            <div>
                                <ol class="aui-nav aui-nav-pagination" style="text-align:center;" id="issuenav">

                                </ol>
                            </div>
                            <div style="margin-top:20px;">
                                <div style="margin-left: 10px;">
                                    <a href="#" class="jira-btn-sm" style="float:right; margin-left: 10px;" id="btnaddlinkissue" ng-click="getStories()">
                                        <span class="aui-icon aui-icon-small aui-iconfont-add-circle"
                                            style="margin-left:5px;"></span>$i18n.getText("designing-view.label.add-linkedissue")
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--Linked Issue Pane end-->
                </div>
        </section>
        <section id="bc-section" class="aui-page-panel-content" style="display:none;">
