<div class="test-results-view" style="background:white;">
    <div class="test-results-summary-container">
        <div class="test-results-summary-details">
            <div class="aui-nav-heading" title="$i18n.getText('reportsview.div.aui-nav-heading.tooltip.stuff')" style="margin: 5px 0;">
                <strong style="color: #000;">$i18n.getText("reportsview.label.current-execution-cycle-details")</strong>
