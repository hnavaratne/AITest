                <div class="aui-page-header-inner" id="business-script-header">
                    <span class="aui-icon aui-icon-large aui-iconfont-bullet-list"></span>
                    <span class="header-title" id="selected-business-Script-titile"></span>
                </div>
            </header>
            <div class="aui-page-panel" style="height:440px;" id="bc-step-view">
                <div class="bc-steps-table-content" id="bc-steps-table-content">
                    <!--<div id="bc-steps-excel-view" class="bc-steps-excel-view"></div>-->
                    	<div class="steps-table-content">
                                        <div class="table-header">
                                            <table>
                                                <thead>
                                                <tr>
                                                    <th class="no">$i18n.getText("reportsview.label.no")</th>
                                                    <th class="desc">$i18n.getText("designing-view.label.step-description")</th>
                                                    <th class="result">$i18n.getText("designing-view.label.expected-result")</th>
                                                    <th class="data">$i18n.getText("all.label.data")</th>
                                                </tr>
                                                </thead>
                                            </table>
                                        </div>
                                        <div class="table-content">
                                            <table context-menu data-target="menu-2" context-menu-close="resetIndex()">
                                                <tbody>
                                                	<tr ng-repeat="bsbdtstep in bsbdtSteps"
                                                    ng-class="{'row-selected':($index  == index)}">
                                                    <td class="no">{{ $index +
                                                        1 }}
                                                    </td>
                                                    <td class="desc" ng-right-click="setSelectedIndex($index)">
                                                        <textarea type="text" ng-model="bsbdtstep.step"
                                                                  class="textarea-desc"
                                                                  style="max-width: 100% !important;height: 100%;font-family: inherit;"
                                                                  ng-click="setClickedIndex($index,0)"
                                                                  ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                        ></textarea>
                                                    </td>
                                                    <td class="result" ng-right-click="setSelectedIndex($index)">
                                                        <textarea class="textarea-result" type="text"
                                                                  ng-model="bsbdtstep.expectedResult"
                                                                  style="max-width: 100% !important;height: 100%;font-family: inherit;"
                                                                     ng-click="setClickedIndex($index,1)"
                                                                  ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                                  ></textarea>
                                                    </td>
                                                    <td class="data" ng-right-click="setSelectedIndex($index)">
                                                        <textarea class="textarea-data"
                                                        ng-click="setBCClickedIndex($index,2,$event)"
                                                                  ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                                  style="max-width: 100% !important;height: 100%;font-family: inherit;" ng-model="bsbdtstep.data" step_id = {{bsbdtstep.id}} ng-change="onChange()"></textarea>
                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td class="no"></td>
                                                    <td class="desc">
                                                        <textarea paste-excel type="text" ng-model="newbsbdtstep.step"
                                                                  class="textarea-desc"
                                                                  ng-click="setClickedIndex(-1,0)"
                                                                  ng-enter="addbsbdtStep()"
                                                                  style="max-width: 100% !important;height: 100%;"
                                                                  ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                        ></textarea>
                                                    </td>
                                                    <td class="result">
                                                        <textarea paste-excel class="textarea-result" type="text"
                                                                    ng-model="newbsbdtstep.expectedResult"
                                                                    ng-enter="addbsbdtStep()"
                                                                    ng-click="setClickedIndex(-1,1)"
                                                                    style="max-width: 100% !important;height: 100%;font-family: inherit;"
                                                                  ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                        ></textarea>

                                                    </td>
                                                    <td class="data">
                                                        <textarea paste-excel class="textarea-data" type="text"
                                                                    ng-model="newbsbdtstep.data"
                                                                    ng-enter="addbsbdtStep()"
                                                                    ng-click="setClickedIndex(-1,2)"
                                                                    style="max-width: 100% !important;height: 100%;font-family: inherit;"
                                                                  ng-mouseup="resize($event)" ng-mousedown="resize($event)"
                                                        ></textarea>
                                                    </td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="dropdown position-fixed" id="menu-2">
                                            <ul class="v-dropdown-menu" role="menu">
                                                <li ng-click="clickedDeleteBdt()"><a class="pointer" role="menuitem" tabindex="1"><span class="aui-icon aui-icon-small aui-iconfont-trash" ></span>$i18n.getText("all.label.delete")</a></li>
                                                <li ng-click="insertNewRowBdt()"><a class="pointer" role="menuitem" tabindex="1"><span class="aui-icon aui-icon-small aui-iconfont-insert-row-after"></span> Insert Row Down</a></li>
